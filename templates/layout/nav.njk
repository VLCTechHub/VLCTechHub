{% set navItems = [
  {name: 'Agenda', uri: '/', id:'events'},
  {name: 'Empleo', uri: '/job/board/',id:'jobs'}
]
%}

<ul class="reset">
  {% for item in navItems %}
    <li class="{% if navItem == item.id %}current{% endif %}"
    >
      <a href="{{item.uri}}">{{ item.name }}</a>
    </li>
  {% endfor %}
</ul>
